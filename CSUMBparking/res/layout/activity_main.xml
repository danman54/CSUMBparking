<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/csumblogo"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:orientation="vertical"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="com.amicorum.csumbparking.MainActivity" >

    <ImageView
        android:id="@+id/imageView1"
        android:layout_height="90dp"
		android:layout_width="wrap_content"
        android:layout_gravity="center_horizontal"
        android:contentDescription="@string/logo1"
        android:src="@drawable/csumb_logo_hdpi" />


	<Button
    	android:id="@+id/parking_button"
    	android:layout_width="wrap_content"
    	android:layout_height="wrap_content"
    	android:layout_marginLeft="106dp"
    	android:layout_marginTop="83dp"
    	android:text="@string/parking_button_text" />
	
	<Button
    	android:id="@+id/button2"
    	android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:layout_marginTop="21dp"
		android:text="Button" />





            <Button
                android:id="@+id/gps_button"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="56dp"
                android:text="@string/gps_button" />


</LinearLayout> <!--  -->

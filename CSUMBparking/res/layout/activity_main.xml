<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/LinearLayout1"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:orientation="vertical"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="com.amicorum.csumbparking.MainActivity" >

    <ImageView
        android:id="@+id/CSUMB_Logo"
        android:layout_width="wrap_content"
        android:layout_height="90dp"
        android:layout_gravity="center_horizontal"
        android:contentDescription="@string/logo1"
        android:src="@drawable/csumbLogoHDPI" />

    <ImageView
        android:id="@+id/backgroundHDPI"
        android:layout_width="match_parent"
        android:layout_width="fill_parent"
        android:layout_height="378dp"
        android:layout_height="fill_parent"
        android:layout_x="0dp"
        android:layout_y="0dp"
        android:adjustViewBounds="true"
        android:contentDescription="@string/background"
        android:gravity="center_vertical"
        android:scaleType="fitXY"
        android:src="@drawable/backgroundHDPI" />

</LinearLayout>